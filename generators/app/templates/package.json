{
  "name": "<%= dizmoName %>",
  "description": "<%= dizmoDescription %>",
  "version": "0.0.0",
  "person": {
    "name": "<%= personName %>",
    "email": "<%= personEmail %>"
  },
  "dizmo": {
    "settings": {
      "bundle-identifier": "<%= bundleId %>",
      "bundle-name": "<%= dizmoName %>",
      "category": "",
      "height": 360,
      "width": 480
    },
    "build": {
      "lint": true,
      "minify": false
    }
  },
  "keywords": [
    "dizmo", "<%= _.kebabCase(dizmoName) %>"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^14.1.0",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-plist": "^0.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-sync": "^0.1.4",
    "gulp-uglify": "^2.0.1",
    "gulp-util": "^3.0.8",
    "gulp-ver": "^0.1.0",
    "gulp-zip": "^4.0.0",
    "javascript-obfuscator": "^0.9.2",
    "lodash": "^4.17.4",
    "rimraf": "^2.5.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0",
    "xtend": "^4.0.1"
  },
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "node ./node_modules/gulp/bin/gulp.js clean",
    "deploy": "node ./node_modules/gulp/bin/gulp.js deploy",
    "lint": "node ./node_modules/gulp/bin/gulp.js lint",
    "make": "node ./node_modules/gulp/bin/gulp.js",
    "test": "exit 0",
    "upload": "node ./node_modules/gulp/bin/gulp.js upload",
    "watch": "node ./node_modules/gulp/bin/gulp.js watch"
  }
}
